{
  "name": "base",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "prepare": "husky install",
    "format": "yarn prettier -w",
    "migrate:test": "DATABASE_URL='file:./db_test.sqlite'  prisma migrate dev",
    "migrate:dev": "prisma migrate dev"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": true,
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "~2.20",
    "@types/lodash": "4.14.168",
    "@types/twitter": "1.7.0",
    "axios": "0.21.1",
    "blitz": "0.34.3",
    "dotenv": "8.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "final-form": "4.20.2",
    "loadsh": "0.0.4",
    "prisma": "~2.20",
    "react": "0.0.0-experimental-6a589ad71",
    "react-dom": "0.0.0-experimental-6a589ad71",
    "react-error-boundary": "3.1.1",
    "react-final-form": "6.5.3",
    "twitter": "1.7.1",
    "types": "0.1.1",
    "typescript": "~4.2",
    "youtube-node": "1.3.3",
    "zod": "1.11.17"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.4",
    "eslint": "7.25.0",
    "husky": "6.0.0",
    "jest-plugin-context": "2.9.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "prettier-plugin-prisma": "0.7.0",
    "pretty-quick": "3.1.0",
    "preview-email": "3.0.4"
  },
  "private": true
}
